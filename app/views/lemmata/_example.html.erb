<li id="<%= path_to_id(example_path(example)) %>"
    class="example column span-24 last">

  <div class="subentry span-24 last">

    <%= render :partial => "examples/margin",
               :locals => {
                 :example => example,
                 :side => 1
               } %>

    <div class="span-6 form1">
      <%= underline example.form1 %>
    </div>

    <div class="span-6 form2">
      <%= underline example.form2 %>
    </div>

    <%= render :partial => "examples/margin",
               :locals => {
                 :example => example,
                 :side => 2
               } %>

    <%= render :partial => 'lemmata/subentry_margin',
               :object => example,
               :locals => { :cmds => %w"edit deleteEntry sortEntries" } %>

  </div>

</li>
